<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.network.akhme.myLittleSocialNetwork">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">

        <activity
            android:name=".presentation.feedActivity.PostsPageActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.view.activities.pages.AddPostActivity">
        </activity>
        <activity
            android:name=".presentation.view.activities.pages.CommentsActivity">
        </activity>
        <activity
            android:name=".zeNewParts.addPostActivity.NewAddPostActivity">
        </activity>

    </application>

</manifest>
